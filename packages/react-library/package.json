{
  "name": "@dytesdk/react-ui-kit",
  "version": "2.3.2-staging.1",
  "description": "Dyte's UI Kit to support easy UI development for React.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dyte-io/ui-kit.git"
  },
  "bugs": {
    "email": "support@dyte.io"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./es": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf dist",
    "compile": "tsup",
    "tsc": "tsc -p .",
    "rollup": "rollup -c",
    "lint": "echo \"No lint script\"",
    "lint:fix": "echo \"No lint:fix script\"",
    "prepublishOnly": "cp package.json package.json.bak && node prepublish.js",
    "postpublish": "mv package.json.bak package.json"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public",
    "tag": "latest"
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@dytesdk/ui-kit": "*",
    "@stencil/react-output-target": "^0.8.2"
  },
  "license": "UNLICENSED"
}
