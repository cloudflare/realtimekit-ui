    "generate": "stencil generate",
    "format": "prettier --write 'src/**/*.{ts,tsx,js,css,html}'"
  },
  "dependencies": {
    "@stencil/core": "^2.12.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^26.6.3",
